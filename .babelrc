{
  "env": {
    "development": {
      "plugins": [
        "react-transform",
        "transform-runtime"
      ],
      "extra": {
        "react-transform": {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }
      }
    }
  }
}